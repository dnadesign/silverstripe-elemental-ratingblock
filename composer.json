{
    "name": "dnadesign/silverstripe-elemental-ratingblock",
    "description": "Rating block for Business.govt's SilverStripe CMS",
    "type": "silverstripe-vendormodule",
    "license": "BSD-3-Clause",
    "require": {
        "silverstripe/framework": "^4.13",
        "silverstripe/graphql": "^4.3",
        "silverstripe/vendor-plugin": "^1.7",
        "colymba/gridfield-bulk-editing-tools": "^4.0"
    },
    "require-dev": {
        "silverstripe/cms": "^5",
        "silverstripe/graphql-devtools": "dev-master"
    },
    "extra": {
        "expose": [
            "client/dist"
        ]
    },
    "scripts": {
        "lint": "phpcs -s code/ tests/",
        "lint-clean": "phpcbf code/ tests/"
    },
    "autoload": {
        "psr-4": {
            "SilverStripe\\AssetAdmin\\": "code/",
            "SilverStripe\\AssetAdmin\\Tests\\": "tests/php/",
            "SilverStripe\\AssetAdmin\\Tests\\Behat\\Context\\": "tests/behat/src/"
        }
    },
    "config": {
        "process-timeout": 600
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}